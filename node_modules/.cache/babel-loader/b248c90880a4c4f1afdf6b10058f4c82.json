{"ast":null,"code":"import Card from \"./CardCar.vue\";\nimport { defineComponent, onMounted, ref, computed } from \"vue\";\nimport api from \"@/services/api\";\nimport TopCards from \"./TopCards.vue\";\nexport default defineComponent({\n  data() {\n    return {\n      count: 8\n    };\n  },\n\n  name: \"ContentItems\",\n  components: {\n    Card,\n    TopCards\n  },\n\n  setup() {\n    const cars = ref([]);\n    const endIndex = ref(8);\n    const filteredCars = computed(() => cars.value.slice(0, endIndex.value));\n\n    const more = () => endIndex.value <= 23 ? endIndex.value = endIndex.value + 8 : endIndex.value;\n\n    const isMobile = () => {\n      if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {\n        return true;\n      } else {\n        return false;\n      }\n    };\n\n    const fetchCars = async () => api.get(\"/\").then(response => cars.value = response.data.resultados.data);\n\n    onMounted(fetchCars);\n    return {\n      cars,\n      filteredCars,\n      more,\n      endIndex,\n      isMobile\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAuDA,OAAOA,IAAP,MAAiB,eAAjB;AACA,SAASC,eAAT,EAA0BC,SAA1B,EAAqCC,GAArC,EAA0CC,QAA1C,QAA0D,KAA1D;AACA,OAAOC,GAAP,MAAgB,gBAAhB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AAEA,eAAeL,eAAe,CAAC;AAC7BM,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE;AADF,KAAP;AAGD,GAL4B;;AAM7BC,MAAI,EAAE,cANuB;AAO7BC,YAAU,EAAE;AAAEV,QAAF;AAAQM;AAAR,GAPiB;;AAQ7BK,OAAK,GAAG;AACN,UAAMC,IAAG,GAAIT,GAAG,CAAC,EAAD,CAAhB;AACA,UAAMU,QAAO,GAAIV,GAAG,CAAC,CAAD,CAApB;AACA,UAAMW,YAAW,GAAIV,QAAQ,CAAC,MAAMQ,IAAI,CAACG,KAAL,CAAWC,KAAX,CAAiB,CAAjB,EAAoBH,QAAQ,CAACE,KAA7B,CAAP,CAA7B;;AAEA,UAAME,IAAG,GAAI,MACXJ,QAAQ,CAACE,KAAT,IAAkB,EAAlB,GACKF,QAAQ,CAACE,KAAT,GAAiBF,QAAQ,CAACE,KAAT,GAAiB,CADvC,GAEIF,QAAQ,CAACE,KAHf;;AAKA,UAAMG,QAAO,GAAI,MAAM;AACrB,UACE,iEAAiEC,IAAjE,CACEC,SAAS,CAACC,SADZ,CADF,EAIE;AACA,eAAO,IAAP;AACF,OANA,MAMO;AACL,eAAO,KAAP;AACF;AACD,KAVD;;AAYA,UAAMC,SAAQ,GAAI,YAChBjB,GAAE,CACCkB,GADH,CACO,GADP,EAEGC,IAFH,CAESC,QAAD,IAAeb,IAAI,CAACG,KAAL,GAAaU,QAAQ,CAAClB,IAAT,CAAcmB,UAAd,CAAyBnB,IAF7D,CADF;;AAKAL,aAAS,CAACoB,SAAD,CAAT;AAEA,WAAO;AAAEV,UAAF;AAAQE,kBAAR;AAAsBG,UAAtB;AAA4BJ,cAA5B;AAAsCK;AAAtC,KAAP;AACD;;AAtC4B,CAAD,CAA9B","names":["Card","defineComponent","onMounted","ref","computed","api","TopCards","data","count","name","components","setup","cars","endIndex","filteredCars","value","slice","more","isMobile","test","navigator","userAgent","fetchCars","get","then","response","resultados"],"sourceRoot":"","sources":["/home/manguita/vuelistradigital/src/components/ContentItems.vue"],"sourcesContent":["<template>\n  <main>\n    <TopCards />\n\n    <div v-for=\"(car, i) in filteredCars\" :key=\"i\">\n      <div v-if=\"!isMobile()\">\n        <div v-if=\"i !== 6\">\n          <Card\n            :foto=\"car.vco_veiculos_foto.url_medium\"\n            :modelo=\"car.marca_titulo\"\n            :titulo=\"car.modelo_titulo\"\n            :versao=\"car.int_versao\"\n            :ano=\"car.int_anomodelofabricacao\"\n            :km=\"car.int_quilometragem\"\n            :cambio=\"car.vco_cambio.titulo\"\n          />\n        </div>\n\n        <div v-if=\"i == 6\">\n          <img\n            class=\"cardDesktop\"\n            v-bind:src=\"require('../assets/cardTroca.png')\"\n          />\n        </div>\n      </div>\n\n      <div v-if=\"isMobile()\">\n        <div v-if=\"i !== 2\">\n          <Card\n            :foto=\"car.vco_veiculos_foto.url_medium\"\n            :modelo=\"car.marca_titulo\"\n            :titulo=\"car.modelo_titulo\"\n            :versao=\"car.int_versao\"\n            :ano=\"car.int_anomodelofabricacao\"\n            :km=\"car.int_quilometragem\"\n            :cambio=\"car.vco_cambio.titulo\"\n          />\n        </div>\n\n        <div v-if=\"i == 1\">\n          <img v-bind:src=\"require('../assets/cardMobile.png')\" />\n        </div>\n      </div>\n    </div>\n\n    <footer>\n      <p>Você já viu {{ endIndex }} de 24 veículos</p>\n      <button @click=\"more\">\n        Carregar Mais <img v-bind:src=\"require('../assets/more.svg')\" />\n      </button>\n    </footer>\n  </main>\n</template>\n\n<script>\nimport Card from \"./CardCar.vue\";\nimport { defineComponent, onMounted, ref, computed } from \"vue\";\nimport api from \"@/services/api\";\nimport TopCards from \"./TopCards.vue\";\n\nexport default defineComponent({\n  data() {\n    return {\n      count: 8,\n    };\n  },\n  name: \"ContentItems\",\n  components: { Card, TopCards },\n  setup() {\n    const cars = ref([]);\n    const endIndex = ref(8);\n    const filteredCars = computed(() => cars.value.slice(0, endIndex.value));\n\n    const more = () =>\n      endIndex.value <= 23\n        ? (endIndex.value = endIndex.value + 8)\n        : endIndex.value;\n\n    const isMobile = () => {\n      if (\n        /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(\n          navigator.userAgent\n        )\n      ) {\n        return true;\n      } else {\n        return false;\n      }\n    };\n\n    const fetchCars = async () =>\n      api\n        .get(\"/\")\n        .then((response) => (cars.value = response.data.resultados.data));\n\n    onMounted(fetchCars);\n\n    return { cars, filteredCars, more, endIndex, isMobile };\n  },\n});\n</script>\n\n<style scoped>\nmain {\n  display: flex;\n  width: 100%;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: space-between;\n}\n@media only screen and (max-width: 600px) {\n  main {\n    display: flex;\n    justify-content: center;\n    align-content: center;\n  }\n}\nfooter {\n  width: 100%;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  align-items: center;\n}\n\nfooter p {\n  font-weight: 600;\n  font-size: 14px;\n}\n\nbutton {\n  color: #7d28f7;\n  text-decoration: none;\n  display: flex;\n  align-items: center;\n  border: none;\n  background: transparent;\n  cursor: pointer;\n}\n\n.cardDesktop {\n  margin-top: 15px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}